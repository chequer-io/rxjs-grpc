{
  "main": "js/index",
  "typings": "js/index",
  "name": "rxjs-grpc",
  "version": "0.2.4",
  "description": "Typesafe gRPC with RxJS in TypeScript",
  "license": "MIT",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/kondi/rxjs-grpc.git"
  },
  "bin": {
    "rxjs-grpc": "./bin/rxjs-grpc"
  },
  "scripts": {
    "start": "node js/cli",
    "build": "tsc",
    "lint": "tslint --project .",
    "prepublish": "npm run build",
    "test": "npm-run-all --parallel test:*",
    "test:lint": "npm run lint",
    "test:format": "npm run format:check",
    "pretest:unit": "npm run build",
    "test:unit": "jest",
    "watch:test": "jest --watch",
    "coverage": "jest --coverage",
    "watch": "tsc --watch",
    "format:check": "prettier --list-different \"{src,examples}/**/*.ts\"",
    "format:apply": "prettier --write \"{src,examples}/**/*.ts\""
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./setup-jest"
  },
  "dependencies": {
    "@grpc/proto-loader": "^0.5.1",
    "bluebird": "^3.5.5",
    "grpc": "^1.24.4",
    "jscodeshift": "^0.6.4",
    "minimist": "^1.2.0",
    "mz": "^2.7.0",
    "protobufjs": "6.8.8",
    "tmp": "^0.1.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.27",
    "@types/glob": "^7.1.1",
    "@types/jasmine": "^3.6.1",
    "@types/jscodeshift": "^0.6.2",
    "@types/minimist": "^1.2.0",
    "@types/mz": "^0.0.32",
    "@types/node": "^12.6.8",
    "@types/tmp": "^0.1.0",
    "glob": "^7.1.4",
    "jasmine": "^3.6.3",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "rxjs": "^6.5.2",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-consistent-codestyle": "^1.15.1",
    "tslint-microsoft-contrib": "^6.2.0",
    "tslint-misc-rules": "^3.5.1",
    "typescript": "^3.5.3"
  },
  "peerDependencies": {
    "rxjs": "^6.5.2"
  }
}
